./booksim examples/mesh44 traffic=transpose injection_rate=0.11
./booksim examples/mesh44 traffic=transpose injection_rate=0.12
./booksim examples/mesh44 traffic=transpose injection_rate=0.13
./booksim examples/mesh44 traffic=transpose injection_rate=0.14
./booksim examples/mesh44 traffic=transpose injection_rate=0.15
